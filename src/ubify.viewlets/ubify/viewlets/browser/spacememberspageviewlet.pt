<table border="0" cellpadding="0" cellspacing="0" width="100%" i18n:domain="cynin">
<tr>
<td>
    <div tal:define="limit_display limit_display|request/limit_display|nothing;
                                        results python:view.batch_results(b_size=limit_display or 20);
                                        over_limit python: limit_display and len(results) > limit_display;
                                        results python: (over_limit and results[:limit_display]) or results;
                                        batch results;
                                        template_id python:'@@spacemembers';
                                        ab limit_display">
    <div class="documentFirstHeading spacemembersicon" tal:condition="results" tal:content="view/title"/>
    <div id="spacemembers">
        <div class="spacemembersavatars" >
            <tal:items tal:repeat="result results">
                    <div class="card avatarlink" tal:define="obj python:result.get('useritem');
                                                  roles python:result.get('roles');
                                                  rolenames python:view.concatroles(roles);
                                                  item_creator obj;
                                                  mtool python:context.portal_membership;
                                                  userid item_creator | nothing;
                                                  userurl string:${here/portal_url}/author/${item_creator};
                                                  objUser python:mtool.getMemberInfo(item_creator);
                                                  fullname objUser/fullname | nothing;
                                                  home python:container.portal_membership.getHomeUrl(userid, verifyPermission=1);
                                                  portrait python: here.portal_membership.getPersonalPortrait(userid);
                                                  portal_url here/portal_url
                                                  "
                                    tal:attributes="rel string:${portal_url}/userinfo?userid=${userid}&rolelist=${rolenames}">
                                    <div class="cardavatar">
                            <img class="border29" src="defaultUser.gif"
                                 alt=""
                                 border="0"
                                 tal:attributes="src portrait/absolute_url" />
                                 </div>
                            <div class="userinfousernamecont"><span class="userinfousername" tal:content="userid">user id</span></div>
                    </div>
            </tal:items>
        </div>
    </div>
    <tal:block>
        <div metal:use-macro="here/batch_macros/macros/navigation" />
    </tal:block>

    </div>
</td>
</tr>
</table>
