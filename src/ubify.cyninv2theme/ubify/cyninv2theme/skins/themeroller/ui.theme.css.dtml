/*component states*/
.ui-default-state {
	border: 1px solid #333333;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	font-weight: normal;
	color: #f1f1f1 !important;
}
.ui-default-state a {
	color: #f1f1f1;
}
.ui-default-state:hover, .ui-hover-state {
	border: 1px solid #0c4b81;
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	font-weight: normal;
	color: #ffffff !important;
}
.ui-hover-state a {
	color: #ffffff;
}
.ui-default-state:active, .ui-active-state {
	border: 1px solid #333333;
	background: #4d4d4d url(/4d4d4d_40x100_textures_03_highlight_soft_80.png) 0 50% repeat-x;
	font-weight: normal;
	color: #ffffff !important;
	outline: none;
}
.ui-active-state a {
	color: #ffffff;
	outline: none;
}
