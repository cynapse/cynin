/*UI accordion*/
.ui-accordion {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
}
/*.ui-accordion-group {*/
/*	margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;*/
/*	border: 1px solid #333333;*/
/*	border-bottom: none;*/
/*}*/
.accordiontab
{
        padding-bottom:.4167em;
}
div.accordionheader
,.ui-accordion-header
{
        background: &dtml-cigray1; url("&dtml-portal_url;/g-gray1-3-s.gif") repeat-x left bottom;
        border-width: 1px;
        border-style:solid none solid solid;
        border-color: &dtml-cigray5;;
        color:&dtml-cigray9;;
        padding:.4167em;
        outline:none;
        text-transform:uppercase;
        cursor:pointer;
        letter-spacing:1px;
}
.verticalnavigationholder
{
        padding:.8333em 0;
}
.accordionheader.expanded
{
        background: &dtml-cigray1; url("&dtml-portal_url;/g-gray1-3-s.gif") repeat-x left bottom;
}
.accordionheader:hover
{
        background: &dtml-ci1co2; url("&dtml-portal_url;/g-blue1-s.gif") repeat-x left top;
        color:&dtml-ci1co4;;
        border-color:&dtml-ci1co3;;
}
.ui-accordion-content
{
	background: &dtml-cigray2;;
	color: #4d4d4d;
	font-size: 1em;
}
.accordiontab dl
,div.navTreeHomeholder
{
        padding:.8333em;
	zoom:100%;
}


/*UI tabs*/
.ui-tabs-nav {
	float: left;
	position: relative;
	/*z-index: 1;*/  /***This is screwing up with clickmenu which is going under ***/
	/*border-right: 1px solid #333333;*/
	bottom: -1px;
        margin:0;
        list-style:none;
}
.ui-tabs-nav li {
        overflow:hidden;
        margin:0;
	/*border: 1px solid #333333;*/
	/*border-right: none;*/
}
.ui-tabs-nav li a {
        padding:0.3636em .75em;
	float: left;
        font-size: 0.9167em;
        line-height: 1.8182em;
        font-weight:normal;
        background: &dtml-cigray1;;
        color:&dtml-cigray8;;
        margin:0 .4545em;
        border-width: 1px;
        border-style:solid solid none solid;
        border-color: transparent;
        text-transform:uppercase;
        letter-spacing:1px;
}
.ui-tabs-nav li.verticaltab a
{
	padding:4px .75em;
        margin:2px 0;
        border-width: 1px;
        border-style:solid none solid solid;
}
.ui-tabs-nav li.horizontaltab.ui-tabs-selected a
{
        padding-bottom:0.4545em;
}
.ui-tabs-nav li a:hover {
	color: #ffffff;
}
.ui-tabs-nav li.ui-tabs-selected {
	border-bottom-color: #4d4d4d;
}
.ui-tabs-nav li.ui-tabs-selected a
,.ui-tabs-nav li.ui-tabs-selected:hover a
{
        border-color: &dtml-cigray5;;
        cursor:default;
        background:&dtml-ciwhite;;
        color:&dtml-cigray9;;
}
.ui-tabs-panel {
	clear:left;
	border-width: 1px;
        border-style: solid none none none;
        border-color:&dtml-cigray5;;
	color: #4d4d4d;
	padding: 1.6667em 0 0 0;
}
.ui-tabs-nav li.verticaltab
{
        margin:0;
        float:none;
}
.ui-tabs-nav li.horizontaltab
{
        border-bottom:1px solid &dtml-cigray5;;
        float:left;
}
.ui-tabs-nav li.horizontaltab.ui-tabs-selected
{
        border-bottom:none;
}
.ui-tabs-nav li.verticaltab
{
        border-right:1px solid &dtml-cigray5;;
}
.ui-tabs-nav li.verticaltab.ui-tabs-selected
{
        border-right:none;
        padding-right:1px;
}
.ui-tabs-nav li.verticaltab.ui-tabs-selected a
{
	left:1px;
	position:relative;
}
.ui-tabs-panel.vertical-tabs-panel
{
        border-style: none none none solid;
        padding:0 0 0 1em;
        min-height: 33.3333em;
}
.vertical-tabs
{
        overflow:hidden;
        margin:0 -1px 0 0;
}
.vertical-tabs li
{
}
.ui-tabs-nav li.verticaltab a
{
        width:80px;
	display:block;
	white-space:nowrap;
}
.vertical-tabs-panel
{
        clear:none;
}
.ui-tabs-hide {
	display: none;/* for accessible hiding: position: absolute; left: -99999999px*/;
}





/*slider*/
.ui-slider {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	background: #f1f1f1 url(/f1f1f1_40x100_textures_01_flat_100.png) 0 0 repeat-x;
	border: 1px solid #4d4d4d;
	height: .8em;
	position: relative;
}
.ui-slider-handle {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	position: absolute;
	z-index: 2;
	top: -3px;
	width: 1.2em;
	height: 1.2em;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	border: 1px solid #333333;
}
.ui-slider-handle:hover {
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	border: 1px solid #0c4b81;
}
.ui-slider-handle-active, .ui-slider-handle-active:hover {
	background: #4d4d4d url(/4d4d4d_40x100_textures_03_highlight_soft_80.png) 0 50% repeat-x;
	border: 1px solid #333333;
}
.ui-slider-range {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	height: .8em;
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	position: absolute;
	border: 1px solid #333333;
	border-left: 0;
	border-right: 0;
	top: -1px;
	z-index: 1;
	opacity:.7;
	filter:Alpha(Opacity=70);
}






/*dialog*/
.ui-dialog {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	background: #f1f1f1 url(/f1f1f1_40x100_textures_01_flat_100.png) 0 0 repeat-x;
	color: #4d4d4d;
	border: 4px solid #4d4d4d;
	position: relative;
}
.ui-resizable-handle {
	position: absolute;
	font-size: 0.1px;
	z-index: 99999;
}
.ui-resizable .ui-resizable-handle {
	display: block;
}
body .ui-resizable-disabled .ui-resizable-handle { display: none; } /* use 'body' to make it more specific (css order) */
body .ui-resizable-autohide .ui-resizable-handle { display: none; } /* use 'body' to make it more specific (css order) */
.ui-resizable-n {
	cursor: n-resize;
	height: 7px;
	width: 100%;
	top: -5px;
	left: 0px;
}
.ui-resizable-s {
	cursor: s-resize;
	height: 7px;
	width: 100%;
	bottom: -5px;
	left: 0px;
}
.ui-resizable-e {
	cursor: e-resize;
	width: 7px;
	right: -5px;
	top: 0px;
	height: 100%;
}
.ui-resizable-w {
	cursor: w-resize;
	width: 7px;
	left: -5px;
	top: 0px;
	height: 100%;
}
.ui-resizable-se {
	cursor: se-resize;
	width: 13px;
	height: 13px;
	right: 0px;
	bottom: 0px;
	background: url(/666666_11x11_icon_resize_se.gif) no-repeat 0 0;
}
.ui-resizable-sw {
	cursor: sw-resize;
	width: 9px;
	height: 9px;
	left: 0px;
	bottom: 0px;
}
.ui-resizable-nw {
	cursor: nw-resize;
	width: 9px;
	height: 9px;
	left: 0px;
	top: 0px;
}
.ui-resizable-ne {
	cursor: ne-resize;
	width: 9px;
	height: 9px;
	right: 0px;
	top: 0px;
}
.ui-dialog-titlebar {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	padding: .5em 1.5em .5em 1em;
	color: #f1f1f1;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	border-bottom: 1px solid #333333;
	font-size: 1em;
	font-weight: normal;
	position: relative;
}
.ui-dialog-title {}
.ui-dialog-titlebar-close {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	background: url(/f1f1f1_11x11_icon_close.gif) 0 0 no-repeat;
	position: absolute;
	right: 8px;
	top: .7em;
	width: 11px;
	height: 11px;
	z-index: 100;
}
.ui-dialog-titlebar-close-hover, .ui-dialog-titlebar-close:hover {
	background: url(/ffffff_11x11_icon_close.gif) 0 0 no-repeat;
}
.ui-dialog-titlebar-close:active {
	background: url(/ffffff_11x11_icon_close.gif) 0 0 no-repeat;
}
.ui-dialog-titlebar-close span {
	display: none;
}
.ui-dialog-content {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	color: #4d4d4d;
	padding: 1.5em 1.7em;
}
.ui-dialog-buttonpane {
	position: absolute;
	bottom: 0;
	width: 100%;
	text-align: left;
	border-top: 1px solid #4d4d4d;
	background: #f1f1f1;
}
.ui-dialog-buttonpane button {
	margin: .5em 0 .5em 8px;
	color: #f1f1f1;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	font-size: 1em;
	border: 1px solid #333333;
	cursor: pointer;
	padding: .2em .6em .3em .6em;
	line-height: 1.4em;
}
.ui-dialog-buttonpane button:hover {
	color: #ffffff;
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	border: 1px solid #0c4b81;
}
.ui-dialog-buttonpane button:active {
	color: #ffffff;
	background: #4d4d4d url(/4d4d4d_40x100_textures_03_highlight_soft_80.png) 0 50% repeat-x;
	border: 1px solid #333333;
}
/* This file skins dialog */
.ui-dialog.ui-draggable .ui-dialog-titlebar,
.ui-dialog.ui-draggable .ui-dialog-titlebar {
	cursor: move;
}







/*datepicker*/
/* Main Style Sheet for jQuery UI date picker */
.ui-datepicker-div, .ui-datepicker-inline, #ui-datepicker-div {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	background: #f1f1f1 url(/f1f1f1_40x100_textures_01_flat_100.png) 0 0 repeat-x;
	border: 4px solid #4d4d4d;
	width: 15.5em;
	padding: 2.5em .5em .5em .5em;
	position: relative;
}
.ui-datepicker-div, #ui-datepicker-div {
	z-index: 9999; /*must have*/
	display: none;
}
.ui-datepicker-inline {
	float: left;
	display: block;
}
.ui-datepicker-control {
	display: none;
}
.ui-datepicker-current {
	display: none;
}
.ui-datepicker-next, .ui-datepicker-prev {
	position: absolute;
	left: .5em;
	top: .5em;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
}
.ui-datepicker-next {
	left: 14.6em;
}
.ui-datepicker-next:hover, .ui-datepicker-prev:hover {
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
}
.ui-datepicker-next a, .ui-datepicker-prev a {
	text-indent: -999999px;
	width: 1.3em;
	height: 1.4em;
	display: block;
	font-size: 1em;
	background: url(/f1f1f1_7x7_arrow_left.gif) 50% 50% no-repeat;
	border: 1px solid #333333;
	cursor: pointer;
}
.ui-datepicker-next a {
	background: url(/f1f1f1_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.ui-datepicker-prev a:hover {
	background: url(/ffffff_7x7_arrow_left.gif) 50% 50% no-repeat;
}
.ui-datepicker-next a:hover {
	background: url(/ffffff_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.ui-datepicker-prev a:active {
	background: url(/ffffff_7x7_arrow_left.gif) 50% 50% no-repeat;
}
.ui-datepicker-next a:active {
	background: url(/ffffff_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.ui-datepicker-header select {
	border: 1px solid #333333;
	color: #f1f1f1;
	background: #808080;
	font-size: 1em;
	line-height: 1.4em;
	position: absolute;
	top: .5em;
	margin: 0 !important;
}
.ui-datepicker-header option:focus, .ui-datepicker-header option:hover {
	background: #0f6bba;
}
.ui-datepicker-header select.ui-datepicker-new-month {
	width: 7em;
	left: 2.2em;
}
.ui-datepicker-header select.ui-datepicker-new-year {
	width: 5em;
	left: 9.4em;
}
table.ui-datepicker {
	width: 15.5em;
	text-align: right;
}
table.ui-datepicker td a {
	padding: .1em .3em .1em 0;
	display: block;
	color: #f1f1f1;
	background: #808080 url(/808080_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
	cursor: pointer;
	border: 1px solid #f1f1f1;
}
table.ui-datepicker td a:hover {
	border: 1px solid #0c4b81;
	color: #ffffff;
	background: #0f6bba url(/0f6bba_40x100_textures_03_highlight_soft_70.png) 0 50% repeat-x;
}
table.ui-datepicker td a:active {
	border: 1px solid #333333;
	color: #ffffff;
	background: #4d4d4d url(/4d4d4d_40x100_textures_03_highlight_soft_80.png) 0 50% repeat-x;
}
table.ui-datepicker .ui-datepicker-title-row td {
	padding: .3em 0;
	text-align: center;
	font-size: .9em;
	color: #4d4d4d;
	text-transform: uppercase;
}
table.ui-datepicker .ui-datepicker-title-row td a {
	color: #4d4d4d;
}
.ui-datepicker-cover {
	display: none;
	display/**/: block;
	position: absolute;
	z-index: -1;
	filter: mask();
	top: -4px;
	left: -4px;
	width: 193px;
	height: 200px;
}











