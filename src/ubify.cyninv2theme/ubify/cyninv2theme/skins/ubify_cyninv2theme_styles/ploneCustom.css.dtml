  /* (en) Centering layout in old IE-versions */
  body { text-align: center; }
  #page_margins { text-align:left; margin: 0 auto; }

  /* (en) Layout: width, background, borders */
  #page_margins
  {
  min-width: 780px;
  }
  #page{ padding: 0px; }

  /* (en) Designing main layout elements */
  #col3 { margin: 0 25%; }
  /**
   * (en) Repositioning content container
   * (de) Neupositionierung der Content Container
   *
   * |-------------------------------|
   * | #header                       |
   * |-------------------------------|
   * | #col3               | #col1   |
   * | flexible            | 25%     |
   * |-------------------------------|
   * | #footer                       |
   * |-------------------------------|
   */

  /* Move #col1 to the right | #col1 nach rechts verschieben */
  #col1 { float:left; width: 210px; }
  #col1_content { padding:0; position:static;}

  /* Hide #col2 | #col2 abschalten */
  #col2, #col2_content { display:none }

  /* Adjust right margin | Rechten Randabstand anpassen */
  #col3 { margin-left:210px; margin-right:0;}
#col3_content
{
        position:static;
        height:100%;
}

#main
{
