/*kss begin*/
.hidden
{
    display:none;
}
#kss-spinner
{
    background:&dtml-ci3co1; none repeat scroll 0 0;
    color:#FFFFFF;
    display:none;
    font-weight:bold;
    height:20px;
    left:auto;
    margin-left:0;
    margin-top:0;
    padding:3px 10px;
    position:fixed;
    right:0;
    top:0;
    width:auto;
    text-transform:uppercase;
}
.kupu-save-message
{
    background:&dtml-ci3co1; none repeat scroll 0 0;
    color:#FFFFFF;
    display:block;
    font-weight:bold;
    height:20px;
    left:auto;
    margin-left:0;
    margin-top:0;
    padding:3px 10px;
    position:fixed;
    right:0;
    top:0;
    width:auto;
    text-transform:uppercase;
    z-index:999;
}

/*kss end*/
/***************************************************************autocomplete *************************/
.ac_results {
    padding: 0px;
    border: 1px solid black;
    background-color: white;
    overflow: hidden;
    z-index: 99999;
}

.ac_results ul {
    width: 100%;
    list-style-position: outside;
    list-style: none;
    padding: 0;
    margin: 0;
}

.ac_results li {
    margin: 0px;
    padding: 2px 5px;
    cursor: pointer;
    display: block;
    text-align:left;
    /* 
    if width will be 100% horizontal scrollbar will apear 
    when scroll mode will be used
    */
    /*width: 100%;*/
    /*font: menu;*/
    /*font-size: 12px;*/
    /* 
    it is very important, if line-height not setted or setted 
    in relative units scroll will be broken in firefox
    */
    /*line-height: 16px;*/
    overflow: hidden;
}

.ac_loading {
    background: white url('&dtml-portal_url;/spinner-w-16.gif') right center no-repeat !important;
}

.ac_odd {
    background-color: #eee;
}

.ac_over {
    background-color: #0A246A;
    color: white;
}
